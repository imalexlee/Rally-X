void initRocks()
{
  mgba_open();
  mgba_printf("init flags %d", 1);
  for (int i = 0; i < 3; i++)
  {
    rocks[i].width = 16;
    rocks[i].height = 16;
    rocks[i].hide = 1;
  }
  rocks[0].x = 264;
  rocks[0].y = 128;
  rocks[1].x = 288;
  rocks[1].y = 320;
  rocks[2].x = 160;
  rocks[2].y = 400;
}
void initRocks2()
{
  for (int i = 0; i < 3; i++)
  {
    rocks[i].hide = 1;
  }
  rocks[0].x = 384;
  rocks[0].y = 176;
  rocks[1].x = 24;
  rocks[1].y = 288;
  rocks[2].x = 248;
  rocks[2].y = 336;
}

void drawRocks()
{

  for (int i = 0; i < 3; i++)
  {

    if (rocks[i].hide)
    {
      if (rocks[i].y - vOff > -rocks[i].height && rocks[i].y - vOff < 160)
      {
        shadowOAM[20 + i].attr0 = ((rocks[i].y - vOff) & 0xFF) | (0<<14);
      }
      else
      {
        shadowOAM[20 + i].attr0 = (2<<8);
      }
      shadowOAM[20 + i].attr1 = ((rocks[i].x - hOff) & 0x1FF) | (0<<14);
      shadowOAM[20 + i].attr2 = (((0) & 0xF) <<12) | (((0)*32+(20)) & 0x3FF) | (((0) & 3) << 10);
    }
    else
    {
      shadowOAM[20 + i].attr0 = (2<<8);
    }
  }
}
